{
  "expo": {
    "name": "yourself",
    "slug": "yourself",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-icon.png",
    "scheme": "yourself",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#373435"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fmacedosantos.yourself"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "158758c5-d0b4-482b-8f48-5a48fc6cefe5"
      }
    }
  }
}
